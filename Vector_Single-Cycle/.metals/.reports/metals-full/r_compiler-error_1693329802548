io.V_out := 0.S


val h = 0.S
val out8 = VecInit(Seq.fill(16)(0.S(8.W)))
val out16 = VecInit(Seq.fill(8)(0.S(16.W)))
val out32 = VecInit(Seq.fill(4)(0.S(32.W)))
val out64 = VecInit(Seq.fill(2)(0.S(64.W)))


