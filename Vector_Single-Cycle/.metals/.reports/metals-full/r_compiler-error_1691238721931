package Vpractice

import chisel3._
import chisel3.util._

class Vlmax extends Module {
    val io = IO (new Bundle{
        val lmul = Input ( UInt (3.W ) )
        val sew = Input ( UInt (3.W ) )

        val vlmax = Output ( UInt (32.W) )
})

when (io.lmul === 0.U && io.sew === 0.U) {               //lmul = 1, @@
    io.vlmax := 16.U
}.elsewhen (io.lmul === 0.U && io.sew === 0.U) {
    io.vlmax := 1.U
}.elsewhen (io.lmul === 0.U && io.sew === 0.U) {
    io.vlmax := 2.U
}.otherwise {
    io.vlmax := 0.U
}
}


